@model IEnumerable<LawyerTimeTracker.Models.User>
@{
    ViewBag.Title = "All users";
}

<table class="table">
    <tr>
        <td>Email</td><td>First name</td><td>Last name</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Email</td><td>@item.FirstName</td><td>@item.LastName</td>
        </tr>
    }
</table>